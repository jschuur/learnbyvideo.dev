{
  "name": "learnbyvideo.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update:videos:prod": "NODE_ENV=production node backend/updateVideos.mjs $1",
    "update:videos:full:prod": "NODE_ENV=production node backend/updateVideosFull.mjs $1",
    "update:videos:recheck:prod": "NODE_ENV=production node backend/updateVideos.mjs --find-new-videos false",
    "update:channels:prod": "NODE_ENV=production node backend/updateChannels.mjs $1",
    "crawl:channels:prod": "NODE_ENV=production node backend/crawlChannels.mjs $1",
    "update:videos": "node backend/updateVideos.mjs $1",
    "update:videos:full": "node backend/updateVideosFull.mjs $1",
    "update:videos:recheck": "node backend/updateVideos.mjs --find-new-videos false",
    "update:channels": "node backend/updateChannels.mjs $1",
    "crawl:channels": "node backend/crawlChannels.mjs $1",
    "add:channels": "node backend/addChannel.mjs $1",
    "quota": "node backend/quota.mjs",
    "quota:verbose": "node backend/quota.mjs --verbose",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:dump:local": "pg_dump -d learnbyvideodev -Fc -f tmp/dbdump_local_`date +\"%Y-%m-%d_%H:%M:%S\"`.dump",
    "db:dump:local:latest": "dotenv cross-var pg_dump -- -d '%DATABASE_URL%' -Fc -f tmp/dbdump_local_latest.dump",
    "db:dump:prod": "dotenv -e .env.production cross-var -- pg_dump -d '%DATABASE_URL%' -Fc -f tmp/dbdump_prod_latest.dump",
    "db:push:prod": "dotenv -e .env.production cross-var -- pg_restore --no-acl --no-owner --clean -d '%DATABASE_URL%' tmp/dbdump_local_latest.dump",
    "db:push": "npm run db:dump:local:latest && npm run db:push:prod",
    "deploy:backend": "ssh node 'cd learnbyvideo.dev && npm run update:backend'",
    "update:backend": "git checkout backend && git reset --hard && git pull && npm install"
  },
  "dependencies": {
    "@apollo/client": "^3.6.7",
    "@fontsource/inter": "^4.5.11",
    "@fontsource/lato": "^4.5.8",
    "@fontsource/roboto": "^4.5.7",
    "@googleapis/youtube": "^4.2.0",
    "@graphql-tools/schema": "^8.3.14",
    "@prisma/client": "^3.15.2",
    "apollo-server-micro": "^3.8.2",
    "cross-var": "^1.1.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "delay": "^5.0.0",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "lodash-es": "^4.17.21",
    "micro-cors": "^0.1.1",
    "minimost": "^1.2.0",
    "next": "12.1.6",
    "node-fetch": "^3.2.6",
    "pg-tsquery": "^8.4.0",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "pretty-ms": "^8.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-timeago": "^7.1.0",
    "rss-parser": "^3.12.0",
    "sparkline": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.14",
    "prisma": "^3.15.2",
    "tailwindcss": "^3.1.2"
  }
}
