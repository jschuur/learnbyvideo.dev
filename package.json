{
  "name": "learnbyvideo.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update:videos": "NODE_ENV=production node backend/updateVideos.mjs $1",
    "update:channels": "NODE_ENV=production node backend/updateChannels.mjs $1",
    "update:crawl": "node backend/crawl.mjs $1",
    "add:channels": "node backend/add.mjs $1",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:dump:local": "pg_dump -d learnbyvideodev -Fc -f tmp/dbdump_local_`date +\"%Y-%m-%d_%H:%M:%S\"`.dump",
    "db:dump:local:latest": "dotenv cross-var pg_dump -- -d '%DATABASE_URL%' -Fc -f tmp/dbdump_local_latest.dump",
    "db:dump:prod": "dotenv -e .env.production cross-var -- pg_dump -d '%DATABASE_URL%' -Fc -f tmp/dbdump_prod_latest.dump",
    "db:push:prod": "dotenv -e .env.production cross-var -- pg_restore --no-acl --no-owner --clean -d '%DATABASE_URL%' tmp/dbdump_local_latest.dump",
    "db:push": "npm run db:dump:local:latest && npm run db:push:prod"
  },
  "dependencies": {
    "@apollo/client": "^3.6.4",
    "@fontsource/inter": "^4.5.10",
    "@fontsource/lato": "^4.5.8",
    "@fontsource/roboto": "^4.5.7",
    "@googleapis/youtube": "^4.2.0",
    "@graphql-tools/schema": "^8.3.11",
    "@prisma/client": "^3.14.0",
    "apollo-server-micro": "^3.7.0",
    "cross-var": "^1.1.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "delay": "^5.0.0",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "lodash-es": "^4.17.21",
    "micro-cors": "^0.1.1",
    "minimost": "^1.2.0",
    "next": "12.1.6",
    "node-fetch": "^3.2.4",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "pretty-ms": "^7.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-timeago": "^6.2.1",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^5.1.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "prisma": "^3.14.0",
    "tailwindcss": "^3.0.24"
  }
}
