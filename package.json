{
  "name": "learnbyvideo.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "find:videos": "node backend/findVideos.mjs $1",
    "find:videos:prod": "NODE_ENV=production node backend/findVideos.mjs $1",
    "find:videos:recheck": "node backend/findVideos.mjs --find-new-videos false",
    "find:videos:recheck:prod": "NODE_ENV=production node backend/findVideos.mjs --find-new-videos false",
    "update:videos": "node backend/updateVideos.mjs $1",
    "update:videos:prod": "NODE_ENV=production node backend/updateVideos.mjs $1",
    "update:channels:prod": "NODE_ENV=production node backend/updateChannels.mjs $1",
    "update:videos:recent": "node backend/updateVideos.mjs -m 2",
    "update:videos:recent:prod": "NODE_ENV=production node backend/updateVideos.mjs -m 2",
    "crawl:channels:prod": "NODE_ENV=production node backend/crawlChannels.mjs $1",
    "update:channels": "node backend/updateChannels.mjs $1",
    "crawl:channels": "node backend/crawlChannels.mjs $1",
    "add:channels": "node backend/addChannel.mjs $1",
    "quota": "node backend/quota.mjs",
    "quota:verbose": "node backend/quota.mjs --verbose",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:dump:local": "pg_dump -d learnbyvideodev -Fc -f tmp/dbdump_local_`date +\"%Y-%m-%d_%H:%M:%S\"`.dump",
    "db:dump:local:latest": "dotenv cross-var pg_dump -- -d '%DATABASE_URL%' -Fc -f tmp/dbdump_local_latest.dump",
    "db:dump:prod": "dotenv -e .env.production cross-var -- pg_dump -d '%DATABASE_URL%' -Fc -f tmp/dbdump_prod_latest.dump",
    "db:push:prod": "dotenv -e .env.production cross-var -- pg_restore --no-acl --no-owner --clean -d '%DATABASE_URL%' tmp/dbdump_local_latest.dump",
    "db:push": "npm run db:dump:local:latest && npm run db:push:prod",
    "deploy:backend": "ssh node 'cd learnbyvideo.dev && npm run update:backend'",
    "update:backend": "git checkout backend && git reset --hard && git pull && npm install && npx prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@fontsource/inter": "^5.0.3",
    "@fontsource/lato": "^5.0.3",
    "@fontsource/roboto": "^5.0.3",
    "@googleapis/youtube": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@next/bundle-analyzer": "^13.4.7",
    "@prisma/client": "^4.16.1",
    "@tanstack/react-query": "^4.29.18",
    "apollo-server-micro": "^3.12.0",
    "cross-var": "^1.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "delay": "^6.0.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-scalars": "^1.22.2",
    "lodash-es": "^4.17.21",
    "micro-cors": "^0.1.1",
    "minimost": "^1.2.0",
    "next": "13.4.7",
    "node-fetch": "^3.3.1",
    "pg-tsquery": "^8.4.1",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "pretty-ms": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-timeago": "^7.1.0",
    "rss-parser": "^3.13.0",
    "serialize-javascript": "^6.0.1",
    "sparkline": "^0.2.0",
    "spinners-react": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "babel-plugin-import-graphql": "^2.8.1",
    "babel-plugin-superjson-next": "^0.4.5",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.24",
    "prisma": "^4.16.1",
    "tailwindcss": "^3.3.2"
  }
}
